{
    "name": "@paychex/adapter-xhr",
    "version": "3.0.0",
    "description": "A data adapter that uses the XMLHttpRequest object in web browsers to convert a Request into a Response.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.mjs",
    "browser": "dist/umd/paychex.adapter-xhr.js",
    "files": [
        "dist",
        "docs",
        "types"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/paychex/adapter-xhr.git"
    },
    "bugs": {
        "url": "https://github.com/paychex/adapter-xhr/issues"
    },
    "homepage": "https://paychex.github.io/adapter-xhr/",
    "scripts": {
        "test": "mocha",
        "coverage": "c8 --clean=true --exclude=spec --reporter=html mocha",
        "docs": "typedoc",
        "build": "rollup -c",
        "postbuild": "tsc --declaration --emitDeclarationOnly --declarationDir types",
        "clean": "rimraf dist docs coverage types",
        "preversion": "npm run clean && npm run docs && npm run build",
        "prepublishOnly": "c8 --clean=true --exclude=spec --reporter=html --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 mocha --recursive"
    },
    "keywords": [
        "paychex",
        "adapter",
        "xhr",
        "xmlhttprequest",
        "data"
    ],
    "license": "Apache-2.0",
    "author": "Paychex, Inc.",
    "contributors": [
        {
            "name": "Dan Barnes",
            "email": "dbarnes1@paychex.com"
        }
    ],
    "engines": {
        "node": ">= 13.2.0",
        "npm": ">= 7.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/preset-env": "^7.13.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@rollup/plugin-typescript": "^8.3.0",
        "@types/lodash-es": "^4.17.5",
        "@types/mocha": "^9.1.0",
        "c8": "^7.3.5",
        "expect": "^26.0.1",
        "mocha": "^8.3.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.39.1",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "rollup-plugin-terser": "^7.0.2",
        "ts-node": "^10.4.0",
        "tslib": "^2.3.1",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    },
    "peerDependencies": {
        "@paychex/core": ">= 3.0.0",
        "lodash": ">= 4.17.21"
    }
}
